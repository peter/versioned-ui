{
  "name": "versioned-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "$(yarn bin)/vue-cli-service serve --open",
    "build": "$(yarn bin)/vue-cli-service build",
    "lint": "$(yarn bin)/vue-cli-service lint",
    "start": "node server.js",
    "heroku-postbuild": "yarn install --production=false && yarn build"
  },
  "dependencies": {
    "vue": "2.5.13",
    "vue-router": "3.0.1",
    "vuex": "3.0.1",
    "express": "4.16.2",
    "axios": "0.17.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.1",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.1",
    "@vue/cli-service": "^3.0.0-beta.1",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
